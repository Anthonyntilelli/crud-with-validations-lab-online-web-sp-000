      <th>Genre</th>
      <th>Artist name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |s| %>
      <tr>
        <td><%= s.title %></td>
        <td><%= s.release_year %></td>
        <td><%= s.genre %></td>
        <td><%= s.artist_name %></td>
        <td>
          <%= link_to 'View Song', song %> |
